<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="dashboard_service_provider_inherited" inherit_id="g2p_agent_portal_base.home_page">
        <xpath expr="//div[@class='container-adjuster']" position="inside">
        <link rel="stylesheet" href="/g2p_reimbursement_portal/static/src/css/portal.css" />
        <div class="card-container" style="justify-content: space-evenly">
            <div class="card">
                <h5 class="card-heading">Reimbursements</h5>
                <hr />
                <ul class="card-block">
                    <a href="/portal/reimbursement/voucher">
                        <li>
                            <div class="icon-image">
                                <img
                                        src="/g2p_reimbursement_portal/static/src/img/person_filled_FILL0_wght400_GRAD0_opsz48.png"
                                        class="img"
                                    />
                            </div>
                            <div>
                                <a href="/portal/reimbursement/voucher">Reimbursements</a>
                                <p>Details</p>
                            </div>
                        </li>
                    </a>
                </ul>
                <div class="card-foot">
                    <a
                            href="/portal/reimbursement/voucher"
                            class="justify-content-center viewprograms group"
                        >View All</a>
                </div>
            </div>
        </div>
        </xpath>
    </template>
</odoo>
