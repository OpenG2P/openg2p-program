<odoo>

    <record id="view_spp_program_reimbursement_form" model="ir.ui.view">
        <field name="name">view_spp_program_reimbursement_form</field>
        <field name="model">g2p.program</field>
        <field name="inherit_id" ref="g2p_program_reimbursement.view_program_reimbursement_form" />
        <field name="priority" eval="104" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='file_size_ssp']" position="replace">
                <field name="file_size_spp" string="Service Provider Portal" />
            </xpath>
        </field>
    </record>

</odoo>
